{
  "name": "ant-design-pro",
  "version": "5.2.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi generate tmp",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "cross-env UMI_ENV=dev umi dev"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.20.4",
    "@ant-design/pro-descriptions": "^1.10.73",
    "@ant-design/pro-form": "^1.67.3",
    "@ant-design/pro-layout": "^6.38.2",
    "@ant-design/pro-table": "^2.74.3",
    "@umijs/route-utils": "^2.1.1",
    "ahooks": "^3.3.10",
    "antd": "^4.20.4",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "omit.js": "^2.0.2",
    "rc-menu": "^9.6.0",
    "rc-util": "^5.21.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet-async": "^1.3.0",
    "umi": "^3.5.23"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/history": "^5.0.0",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-helmet": "^6.1.5",
    "@umijs/fabric": "^2.10.2",
    "@umijs/plugin-access": "^2.5.1",
    "@umijs/plugin-antd": "^0.15.0",
    "@umijs/plugin-dva": "^0.13.2",
    "@umijs/plugin-esbuild": "^1.4.1",
    "@umijs/plugin-helmet": "^1.1.4",
    "@umijs/plugin-initial-state": "^2.4.0",
    "@umijs/plugin-layout": "^0.18.2",
    "@umijs/plugin-locale": "^0.16.0",
    "@umijs/plugin-model": "^2.6.2",
    "@umijs/plugin-request": "^2.9.0",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.4.0",
    "detect-installer": "^1.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "react-dev-inspector": "^1.8.0",
    "stylelint": "^14.8.2",
    "typescript": "^4.6.4",
    "umi-serve": "^1.9.10"
  },
  "engines": {
    "node": ">=16.5.0"
  },
  "packageManager": "yarn@3.2.0"
}
